	ubiargs_default () {
		fw_setenv ubi_args 'ubi_args_default'
	}

	ubiargs_tmpfs () {
		fw_setenv ubi_args 'ubi_args_tmpfs'
	}

	nc_off () {
		fw_setenv preboot
	}

	nc_on () {
		fw_setenv preboot 'run nc_test nc_start'
	}

	boot_fast () {
		fw_setenv bootcmd '$bootcmd_fast'
	}

	boot_normal () {
		fw_setenv bootcmd '$bootcmd_go'
	}

	ethaddr () {
		fw_setenv ethaddr '$1'
	}
