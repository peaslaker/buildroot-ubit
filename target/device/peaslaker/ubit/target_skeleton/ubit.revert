(
	nanddump -nos 0x80000 -l 0x20000 -f /tmp/part1 /dev/mtd0 
	nanddump -nos 0xc0000 -l 0x40000 -f /tmp/part2 /dev/mtd0
	cat /tmp/part1 /tmp/part2 > /tmp/tmp.kwb 
	for v in /*.kwb.md5; do
		cat /tmp/tmp.kwb | md5sum -c $v -s && {
		echo UBIT:REVERT revert u-boot matches $v
		exit 1
		}
	done
) || {
	set - tmp/tmp
	. /ubit.uboot
	}

